                                @Html.Partial("BoxLogin")
                            </ul>
                            <ul class="nav navbar-nav navbar-right">
                                <li>
                                    <a class="page-scroll" href="/">Trang chủ</a>
                                </li>
                                @*<li>
                                    <a class="page-scroll" href="#services">Tính năng</a>
                                </li>
                                <li>
                                    <a class="page-scroll" href="#tableSell">Bảng giá</a>
                                </li>
                                <li>
                                    <a class="page-scroll" href="#appFb">Cài đặt App</a>
                                </li>*@
                                <li>
                                    <a class="page-scroll" href="#footer">Hỗ trợ</a>
                                </li>
                            </ul>
                        </div>
                        <!-- /.navbar-collapse -->
                    </div>
                    <!-- /.container -->
                </nav>
                <!-- Section Intro Slider
            ================================================== -->
                <div id="carousel-example-generic" class="carousel intro slide">
                    <div class="carousel-inner" role="listbox" ng-controller="SearchController">
                        <!-- First slide -->
                        <div class="item active" style="background-image:url(/Content/develop/images/banner.jpg)">
                            <div class="carousel-caption">
                                <h1 data-animation="animated bounceInUp"> Phân tích fanpage, group mà bạn quan tâm</h1>
                                <h2 data-animation="animated bounceInDown">
                                    Với các chỉ số KPI cụ thể sẽ giúp bạn nắm bắt được thị hiếu , tầm ảnh hưởng và các phản hồi của người dùng. Bạn có thể dễ dàng theo dõi thông tin của đối thủ cạnh tranh và tăng phạm vi tiếp cận cho nội dung của mình trên MXH facebook hơn nữa.
                                </h2>
                                <h3>Bắt đầu phân tích của bạn bây giờ</h3>
                                <div class="form-control form-search" data-animation="animated fadeInLeft">
                                    <input class="inputSearch" type="text" placeholder="Nhập tên hoặc đường dẫn" input-enter="openSearch();" ng-model="textSearch" style="width:70%;">
                                    <div class="selectP" style="position: relative">
                                        <select class="singleSelect" name="selectSearch" ng-model="typeSearch" ng-init="typeSearch='0'" style="-webkit-appearance: none;-moz-appearance: none; width: 100%">
                                            <option value="0" selected>Page</option>
                                            <option value="1">Group</option>
                                        </select>
                                        <span class="fa fa-chevron-down icon-select" aria-hidden="true"></span>
                                    </div>
                                    <a class="btn btn-ghost btn-lg btn-primary" ng-click="openSearch();" style="width:10%;">
                                        <i class="fa fa-search"></i>
                                    </a>
                                </div>
                            </div>
                        </div>
                        <!-- /.item -->
                    </div>
                    <!-- /.carousel-inner -->
                    <!-- Controls (currently displayed none from styleMainHome.css)-->
                    <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
                        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
                        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
                <!-- /.carousel -->
                <!-- Button trigger modal -->
                <!-- Modal -->
                <div id="loginModal" class="modal fade popupLogin" role="dialog">
                    <div class="modal-dialog">
                        <!-- Modal content-->
                        <div class="modal-content">
                            <div class="modal-body">
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                                <div class="col-lg-6 info-PM">
                                    <a href="#" class="logo-pop dpib ovh" tabindex="-1">
                                        <img src="/Content/develop/images/logo-fb.sale-1.png" alt="Salesmanager" title="Sales Manager">
                                    </a>
                                    <h1>Để quản lý bán hàng mọi lúc, mọi nơi, trên mọi thiết bị.</h1>
                                    <p>
                                        Giải pháp bán hàng qua SalesManager toàn diện, hiệu quả. Quản lý danh sách khách hàng, tin nhắn,
                                        bình
                                        luận, tình trạng đơn hàng, gửi hàng nhanh chóng trên web và di động
                                    </p>
                                    <!--<images src="/Content/images/desk.png">-->
                                </div>
                                <form class="col-lg-6 loginbox">
                                    <section class="loginFr ovh vi-VN">
                                        <h2> Đăng nhập</h2>
                                        <!-- Tab panes -->
                                        <div class="tab-content import-Info">
                                            <div role="tabpanel" class="tab-pane active" id="home">
                                                <form class="form-horizontal">
                                                    <div class="form-group">
                                                        <div class="w100pt input-group">
                                                            <div class="label">
                                                                <i class="fa fa-envelope" aria-hidden="true"></i>
                                                            </div>
                                                            <input type="email" class="form-control" id="txtLoginEmail"
                                                                   placeholder="Email: fbsale@gmail.com">
                                                        </div>
                                                    </div>
                                                    <div class="form-group">
                                                        <div class="w100pt input-group">
                                                            <div class="label">
                                                                <i class="fa fa-key" aria-hidden="true"></i>
                                                            </div>
                                                            <input type="password" class="form-control" id="txtLoginPassword" input-enter="login();"
                                                                   placeholder="* * * * *">
                                                        </div>
                                                    </div>
                                                    <div class="checkbox left w100pt">
                                                        <input id="checkbox1" class="styled" type="checkbox">
                                                        <label for="checkbox1">Ghi nhớ mật khẩu</label>
                                                    </div>
                                                    <div class="form-group boxBt">
                                                        <button type="submit" class="btn bt-login bg-green clfff" ng-click="login();">
                                                            Đăng nhập
                                                        </button>
                                                        <a href="#" class="left cl-green bt-foget">Quên mật khẩu ?</a>
                                                        <a href="#" class="right cl-green bt-resgiter">Đăng ký tại đây !</a>
                                                        <div class="left w100pt logigFacebook">
                                                            <h3>Hoặc đăng nhập bằng tài khoản sau:</h3>
                                                            <button type="submit" class="btn bt-login-face" ng-click="loginFacebook();">
                                                                Đăng nhập Facebook
                                                            </button>
                                                        </div>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </section>
                                </form>
                                <div class="clearfix"></div>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                </div>         
            

            <div id="infoModal" class="modal fade popupLogin" role="dialog" tabindex="-1" data-keyboard="false">
                <div class="modal-dialog">
                    <!-- Modal content-->
                    <div class="modal-content">
                        <div class="modal-body">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                            <div class="col-lg-6 info-PM">
                                <a href="#" class="logo-pop dpib ovh" tabindex="-1">
                                    @*<img src="dist/img/logo-fb.sale-1.png" alt="Salesmanager" title="Sales Manager">*@
                                    <img src="#" alt="Salesmanager" title="Sales Manager">
                                </a>
                                <h1 translate="index.login_popup.popup_text1"></h1>
                                <p translate="index.login_popup.popup_text2">

                                </p>
                                <!--<img src="develop/img/desk.png">-->
                            </div>
                            <form class="col-lg-6 loginbox">
                                <section class="loginFr ovh vi-VN">

                                    <h2 translate="common.yourinfo"></h2>
                                    <!-- Tab panes -->
                                    <div class="tab-content import-Info">
                                        <div role="tabpanel" class="tab-pane active" id="home">
                                            <form class="form-horizontal">
                                                <div class="form-group">
                                                    <div class="w100pt input-group">
                                                        <div class="label">
                                                            <i class="fa fa-user" aria-hidden="true"></i>
                                                        </div>
                                                        <input type="text" class="form-control" id="txtFullName" ng-model="userInfo.FullName" ng-required="true"
                                                               placeholder="{{ 'common.full_name' | translate }}">
                                                        <span style="color:red;font-size:24px;line-height:44px;" class="err_msg" ng-show="userInfo.FullName == '' || userInfo.FullName == undefined">*</span>
                                                    </div>
                                                </div>

                                                <div class="form-group">
                                                    <div class="w100pt input-group">
                                                        <div class="label">
                                                            <i class="fa fa-phone" aria-hidden="true"></i>
                                                        </div>
                                                        <input type="text" class="form-control" id="txtPhone" ng-model="userInfo.Phone" ng-required="true" ng-pattern="/^(09|08|012|016|84)(\d{8,9})$/"
                                                               placeholder="{{ 'common.phone' | translate }}">
                                                        <span style="color:red;font-size:24px;line-height:44px;" class="err_msg ng-hide" ng-show="(userInfo.Phone.$invalid && !userInfo.Phone.$pristine) || userInfo.Phone == '' || userInfo.Phone == undefined">*</span>
                                                    </div>
                                                </div>

                                                <div class="form-group">
                                                    <div class="w100pt input-group">
                                                        <div class="label">
                                                            <i class="fa fa-phone" aria-hidden="true"></i>
                                                        </div>
                                                        <input type="email" class="form-control" id="txtEmail" ng-model="userInfo.RegEmail" ng-required="true" ng-pattern="/^[a-zA-Z0-9.!#$%&’*+/=?^_`{|}~-]+@("@")[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)*$/"
                                                               placeholder="{{ 'common.email' | translate }}">
                                                        <span style="color:red;font-size:24px;line-height:44px;" class="err_msg ng-hide" ng-show="(userInfo.RegEmail.$invalid && !userInfo.RegEmail.$pristine) || userInfo.RegEmail == '' || userInfo.RegEmail == undefined">*</span>
                                                    </div>
                                                </div>

                                                <div class="form-group">
                                                    <div class="w100pt input-group">
                                                        <div class="label">
                                                            <i class="fa fa-map-marker" aria-hidden="true"></i>
                                                        </div>
                                                        <input type="text" class="form-control" id="txtAddress" ng-model="userInfo.Address"
                                                               placeholder="{{ 'common.address' | translate }}">
                                                    </div>
                                                </div>

                                                <div class="form-group">
                                                    <div class="w100pt input-group">
                                                        <div class="label">
                                                            <i class="fa fa-home" aria-hidden="true"></i>
                                                        </div>
                                                        <input type="text" class="form-control" id="txtShopName" ng-model="userInfo.ShopName"
                                                               placeholder="{{ 'common.shop_name' | translate }}">
                                                    </div>
                                                </div>

                                                <div class="form-group boxBt">
                                                    <button type="button" class="btn bt-login bg-green clfff" ng-click="updateInfo();">
                                                        {{ 'common.continue' | translate }}
                                                    </button>
                                                </div>
                                            </form>
                                        </div>

                                    </div>


                                </section>
                            </form>
                            <div class="clearfix"></div>
                        </div>
                        <div class="clearfix"></div>
                    </div>

                </div>
            </div>
            <!-- Section Services
        ================================================== -->
            <section id="services" class="services">
                <div class="container">
                    <div class="col-md-12">
                        <div class="select select--white right">
                            <select class="form-control selectpicker" ng-model="crr_country" @*ng-init="q.selectCountry='0'"*@ ng-change="onQueryChange()" ;>
                                <option value="0">Tất cả</option>
                                <option ng-repeat="item in listCountry" value="{{ item.code }}">{{ item.name }}</option>
                            </select>
                        </div>
                        <label class="right filter-title" style="margin: 3px 20px;">Quốc gia</label>
                    </div>
                </div>
                <div class="container">
                    <div class="row">
                        <!-- begin 1st row -->
                        <div class="section-item col-lg-4 col-md-6 text-center">
                            <div class="service-box">
                                <div class="iconbox">
                                    <img src="/Content/develop/images/icon-01.png">
                                </div>
                                <h3>TOP page</h3>
                                <p class="text-muted">
                                    Tiêu chí đánh giá chủ yếu dựa trên số lượt like page . Bạn có thể theo dõi và cập nhật được xu hướng mà xã hội đang quan tâm qua thứ tự xếp hạng các page.
                                </p>
                                <div class="boxlistItem left w100pt">
                                    <ul class="list-item lable left w100pt">
                                        <li>
                                            <span class="stt">STT</span>
                                            <h1> Page </h1>
                                            <span class="like">Likes</span>
                                        </li>
                                    </ul>
                                    <ul class="list-item listMore top-page left w100pt">
                                        <li ng-class-odd="'arrow odd'" ng-class-even="'arrow odd bge'" ng-repeat="listPage in listTopPages">
                                            <span class="stt number"> {{ ((page-1) * page_size) + $index + 1 }} </span>
                                            <h1 class="info-item">
                                                <a href="/detail/{{ listPage.page_id }}/page.html" title="{{ listPage.page_name }}">
                                                    <img class="imgItem" ng-src="https://graph.facebook.com/{{listPage.facebook_id}}/picture?type=square"><span>{{ listPage.page_name }}</span>
                                                </a>
                                                <span class="like1">{{ listPage.fan_count | formatPrice }}</span>
                                            </h1>
                                        </li>
                                    </ul>
                                    <a class="" href="/allpage.html">Xem tất cả</a>
                                </div>
                            </div>
                        </div>
                        <!-- begin 2nd row -->
                        <div class="section-item col-lg-4 col-md-6 text-center">
                            <div class="service-box">
                                <div class="iconbox">
                                    <img src="/Content/develop/images/icon-02.png">
                                </div>
                                <h3>Tốc độ tăng like trên ngày</h3>
                                <p class="text-muted">
                                    Thống kê lượt like tăng trong khoảng thời gian là một ngày để đánh giá xem page nào hoạt động có hiệu quả và tiếp cận tới nhiều người dùng nhất.
                                </p>
                                <div class="boxlistItem left w100pt">
                                    <ul class="list-item lable left w100pt">
                                        <li>
                                            <span class="stt">STT</span>
                                            <h1> Page </h1>
                                            <span class="like">Likes</span>
                                        </li>
                                    </ul>
                                    <ul class="list-item listMore tang-like left w100pt">
                                        <li ng-repeat="listPageFan in listTopPagesFan">
                                            <span class="stt number"> {{ ((page-1) * page_size) + $index + 1 }} </span>
                                            <h1 class="info-item">
                                                <a href="/detail/{{ listPageFan.page_id }}/page.html" title="{{ listPageFan.page_name }}">
                                                    <img class="imgItem" ng-src="https://graph.facebook.com/{{listPageFan.facebook_id}}/picture?type=square"><span>{{ listPageFan.page_name }}</span>
                                                </a>
                                                <span class="like1">{{ listPageFan.fan_increase_yesterday | formatPrice }}</span>
                                            </h1>
                                        </li>
                                    </ul>
                                    <a class="" href="/allpage.html">Xem tất cả</a>
                                </div>
                            </div>
                        </div>
                        <div class="section-item col-lg-4 col-md-6 text-center">
                            <div class="service-box">
                                <div class="iconbox">
                                    <img src="/Content/develop/images/icon-pageNew1.png">
                                </div>
                                <h3>page mới</h3>
                                <p class="text-muted">
                                    Thống kê danh sách các page được mọi người tìm kiếm gần đây nhất.
                                </p>
                                <div class="boxlistItem left w100pt">
                                    <ul class="list-item lable left w100pt">
                                        <li>
                                            <span class="stt">STT</span>
                                            <h1> Page </h1>
                                            <span class="like">Likes</span>
                                        </li>
                                    </ul>
                                    <ul class="list-item listMore top-page left w100pt">
                                        <li ng-class-odd="'arrow odd'" ng-class-even="'arrow odd bge'" ng-repeat="item in listNewPages">
                                            <span class="stt number"> {{ ((page-1) * page_size) + $index + 1 }} </span>
                                            <h1 class="info-item">
                                                <a href="/detail/{{ item.page_id }}/page.html" title="{{ item.page_name }}">
                                                    <img class="imgItem" ng-src="https://graph.facebook.com/{{item.facebook_id}}/picture?type=square"><span>{{ item.page_name }}</span>
                                                </a>
                                                <span class="like1">{{ item.fan_count | formatPrice }}</span>
                                            </h1>
                                        </li>
                                    </ul>
                                    <a class="" href="/allpage.html">Xem tất cả</a>
                                </div>
                            </div>
                        </div>
                        <div class="section-item col-lg-4 col-md-6 text-center hidden">
                            <div class="service-box">
                                <div class="iconbox">
                                    <img src="/Content/develop/images/icon-03.png">
                                </div>
                                <h3>Group nhiều thành viên nhất</h3>
                                <p class="text-muted">
                                    Thống kê số lượng thành viên của các nhóm trên facebook.
                                </p>
                                <div class="boxlistItem left w100pt">
                                    <ul class="list-item lable left w100pt">
                                        <li>
                                            <span class="stt">STT</span>
                                            <h1> Group </h1>
                                            <span class="like" style="width: 61px;">New</span>
                                            <span class="like" style="width: 81px; border-right: none;">Member</span>
                                        </li>
                                    </ul>
                                    <ul class="list-item listMore ground left w100pt">
                                        <li ng-repeat="listGroup in listTopGroups">
                                            <span class="stt number">{{ ((page-1) * page_size) + $index + 1 }}</span>
                                            <h1 class="info-item">
                                                <a href="/detail/{{ listGroup.group_id }}/group.html" title="{{ listGroup.group_name }}">
                                                    <img class="imgItem" ng-src="{{listGroup.group_avatar}}"><span>{{ listGroup.group_name }}</span>
                                                </a>
                                                <span class="like1" style="right: 51px; width: 81px; padding: 0px 5px;">{{ listGroup.members_count | formatPrice }}</span>
                                                <span class="like1" style="width: 51px;">{{ listGroup.member_increase_yesterday | formatPrice }}</span>
                                            </h1>
                                        </li>
                                    </ul>
                                    <a class="" href="/allgroup.html">Xem tất cả</a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- end 1st row -->
                    <br />
                    <div class="row">
                        <!-- begin 1st row -->
                        <div class="section-item col-lg-4 col-md-6 text-center">
                            <div class="service-box">
                                <div class="iconbox">
                                    <img src="/Content/develop/images/icon-03.png">
                                </div>
                                <h3>Top Group</h3>
