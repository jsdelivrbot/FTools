@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Login - FTOOL - Analytic Facebook</title>
    <meta name="description" content="FTOOL - Phân tích dữ liệu fanpage và group trên facebook">
    <meta name="author" content="ftool - eyeplus online ">

    <!-- Bootstrap Core CSS -->
    <link rel="stylesheet" href="/Content/dist/css/bootstrap.min.css" type="text/css">
    <!-- Custom Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800">

    <link rel="stylesheet" href="/Content/dist/css/animate.min.css" type="text/css">
    <link rel="stylesheet" href="/Content/develop/css/styleMainHome.css" type="text/css">
    <link rel="stylesheet" href="/Content/develop/css/reponsize.css" type="text/css">
    <link href="~/Content/develop/css/owl.carousel.css" rel="stylesheet" />
    <link href="~/Content/develop/css/owl.theme.default.css" rel="stylesheet" />
    <link href="/Content/develop/fonts/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-daterangepicker/2.1.13/daterangepicker.min.css" />
    @Styles.Render("~/Content/css")
</head>
<body>
    <div ng-app="FToolApp" ng-cloak>
        <div ng-controller="LoginController" ng-init="">
            <!-- Section Login
            ================================================== -->
            <div class="modal-body">
                <button type="button" class="close hidden" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <div class="col-lg-6 info-PM">
                    <a href="#" class="logo-pop dpib ovh" tabindex="-1">
                        <img src="/Content/develop/images/logo-fb.sale-1.png" alt="Salesmanager" title="Sales Manager">
                    </a>
                    <h1>Để quản lý bán hàng mọi lúc, mọi nơi, trên mọi thiết bị.</h1>
                    <p>
                        Giải pháp bán hàng qua SalesManager toàn diện, hiệu quả. Quản lý danh sách khách hàng, tin nhắn,
                        bình
                        luận, tình trạng đơn hàng, gửi hàng nhanh chóng trên web và di động
                    </p>
                    <!--<images src="/Content/images/desk.png">-->
                </div>
                <form class="col-lg-6 loginbox">
                    <section class="loginFr ovh vi-VN">
                        <h2> Đăng nhập</h2>
                        <!-- Tab panes -->
                        <div class="tab-content import-Info">
                            <div role="tabpanel" class="tab-pane active" id="home">
                                <form class="form-horizontal">
                                    <div class="form-group">
                                        <div class="w100pt input-group">
                                            <div class="label">
                                                <i class="fa fa-envelope" aria-hidden="true"></i>
                                            </div>
                                            <input type="email" class="form-control" id="txtLoginEmail"
                                                   placeholder="Email: fbsale@gmail.com">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="w100pt input-group">
                                            <div class="label">
                                                <i class="fa fa-key" aria-hidden="true"></i>
                                            </div>
                                            <input type="password" class="form-control" id="txtLoginPassword" input-enter="login();"
                                                   placeholder="* * * * *">
                                        </div>
                                    </div>
                                    <div class="checkbox left w100pt">
                                        <input id="checkbox1" class="styled" type="checkbox">
                                        <label for="checkbox1">Ghi nhớ mật khẩu</label>
                                    </div>
                                    <div class="form-group boxBt">
                                        <button type="submit" class="btn bt-login bg-green clfff" ng-click="login();">
                                            Đăng nhập
                                        </button>
                                        <a href="#" class="left cl-green bt-foget">Quên mật khẩu ?</a>
                                        <a href="#" class="right cl-green bt-resgiter">Đăng ký tại đây !</a>
                                        <div class="left w100pt logigFacebook">
                                            <h3>Hoặc đăng nhập bằng tài khoản sau:</h3>
                                            <button type="submit" class="btn bt-login-face" ng-click="">
                                                Đăng nhập Facebook
                                            </button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </section>
                </form>
                <div class="clearfix"></div>
            </div>
            <!-- Section Footer
            ================================================== -->
            <section id="footer" class="footer">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 footer-contant">
                            <div class="footer-info col-lg-3">
                                <a href="#" class="left w100pt logoF"><img src="/Content/develop/images/Logo_Fsale-f.png"></a>
                                <p>
                                    Công cụ tuyệt vời giúp bạn đo lường và phân tích tiềm năng và hiệu quả của Fanpage. Cho phép bạn theo dõi, so sánh và tìm hiểu tất cả thông tin tiềm năng của fanpage trên MXH facebook.
                                </p>
                            </div>
                            <div class="footer-listDM list1 col-lg-2 hide">
                                <h2 class="footer-txt">Danh mục</h2>
                                <ul>
                                    <li><a href="#">Trang chủ</a></li>
                                    <li><a href="#"> Tính năng, đặc điểm</a></li>
                                    <li><a href="#"> Giá cả</a></li>
                                    <li><a href="#"> Tài liệu tham khảo</a></li>
                                    <li><a href="#"> Việc làm</a></li>
                                    <li><a href="#"> Chương trình đối tác</a></li>
                                </ul>
                            </div>
                            <div class="footer-listDM list2 col-lg-3 hide">
                                <h2 class="footer-txt">Chính sách - điều khoản</h2>
                                <ul>
                                    <li><a href="#">Thông báo pháp lý / Liên hệ</a></li>
                                    <li><a href="#">Chính sách bảo mật</a></li>
                                    <li><a href="#">Điều khoản dịch vụ</a></li>
                                </ul>
                            </div>
                            <div class="footer-listDM list3 col-lg-3 hide">
                                <h2 class="footer-txt">Công cụ hữu ích</h2>
                                <ul>
                                    <li><a href="#">Cạnh tranh Good Luck Fairy</a></li>
                                    <li><a href="#">So sánh trang fan hâm mộ</a></li>
                                    <li><a href="#">Danh mục Facebook</a></li>
                                </ul>
                            </div>
                            <div class="footer-share col-lg-1">
                                <!-- Collect the nav links, forms, and other content for toggling -->
                                <ul>
                                    <li>
                                        <a class="page-face" href="https://www.facebook.com/quanlybanhangFSale/"><i class="fa fa-facebook"></i> </a>
                                    </li>
                                    <li>
                                        <a class="page-twitter" href="#"><i class="fa fa-twitter"></i> </a>
                                    </li>
                                    <li>
                                        <a class="page-google" href="#"><i class="fa fa-google"></i></a>
                                    </li>
                                    <li class="likeFace w100pt left">
                                        <a class="btn-Like" href="https://www.facebook.com/quanlybanhangFSale/">
                                            <i class="fa fa-thumbs-up"> Like</i>
                                        </a>
                                        @*<img src="/Content/develop/images/like12.png">*@
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="copyRight">copyRight 2017 by EyePlus Online</div>
            </section>
            <fb app-id='@Session["FBAppId"]'></fb>
        </div>
    </div>
    <!-- jQuery -->
    <script src="/Content/dist/js/jquery.js"></script>
    <script src="/Content/dist/js/bootstrap.min.js"></script>
    <script src="/Content/dist/js/jquery.easing.min.js"></script>
    <script src="/Content/dist/js/wow.min.js"></script>
    <script src="/Content/dist/js/common.js"></script>
    <script src="/Content/develop/slick-master/slick.js" type="text/javascript" charset="utf-8"></script>
    <script src="~/Content/dist/js/owl.carousel.js"></script>

    @Scripts.Render("~/bundles/angularjs")

    <script src="https://www.gstatic.com/firebasejs/4.1.3/firebase.js"></script>
    <script src="https://cdn.firebase.com/libs/angularfire/2.2.0/angularfire.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.9/angular-sanitize.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.6/moment.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-daterangepicker/2.1.13/daterangepicker.min.js"></script>
    <script src="https://fragaria.github.io/angular-daterangepicker/js/angular-daterangepicker.min.js"></script>

</body>
</html>
